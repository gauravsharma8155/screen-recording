{
    "name": "ad blocker",
    "description": "Ad blocker extension ",
    "version": "1.0.0",
    "manifest_version": 3,
    "permissions": [
        "activeTab",
        "Tabs",
        "scripting",
        "storage"
    ],
    "content_script": {
        "js": "content.js",
        "css": "style.css"
    },
    "action": {
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "options_ui": {
        "page": "options.html",
        "open_in_tab": true
    }
}